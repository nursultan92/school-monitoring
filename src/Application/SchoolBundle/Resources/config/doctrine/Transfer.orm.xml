<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\SchoolBundle\Entity\Transfer" table="transfer_locations">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="place" type="string" column="place" length="50"/>
      <one-to-many target-entity="Application\SchoolBundle\Entity\Student" mapped-by="cameFrom" field="studentCame"/>
      <one-to-many target-entity="Application\SchoolBundle\Entity\Student" mapped-by="leftTo" field="studentLeft"/>
  </entity>
</doctrine-mapping>
